<?xml version="1.0" encoding="utf-8"?>
<!--
    SECURITY: Backup rules to prevent sensitive data from being backed up
    Only non-sensitive app data should be included in backups
-->
<full-backup-content>
    <!-- Exclude sensitive data directories -->
    <exclude domain="sharedpref" path="."/>
    <exclude domain="database" path="."/>
    <exclude domain="file" path="."/>
    <exclude domain="external" path="."/>
</full-backup-content>



